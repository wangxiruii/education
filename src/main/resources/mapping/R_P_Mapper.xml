<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.mapper.R_P_Mapper">
    <select id="loadPidByRid" resultType="int" parameterType="int">
        select
        poId
        from
        role_power_mapping
        where
        roleId = #{roleId}
    </select>
</mapper>